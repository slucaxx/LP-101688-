Algoritmo "semnome"

Var
   nome:caracter
   idade:inteiro
   altura:real
Inicio

   escreva("informe seu nome:")
   leia(nome)

   escreva("informe sua idade:")
   leia(idade)

   escreva("informe sua altura:")
   leia(altura)
   
   escreval
   escreval

   escreval("nome ", nome)
   escreval("idade ",idade)
   escreval("altura ",altura, " anos")
Fimalgoritmo